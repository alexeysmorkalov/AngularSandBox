{
  "name": "tsandbox",
  "version": "1.0.2",
  "description": "TS sandbox",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "ts_test": "node_modules/.bin/mocha --require ts-node/register --extensions ts,tsx tests 'tests/**/*.{ts,tsx}'",
    "js_test": "node_modules/.bin/mocha --extensions js,jsx tests 'tests/**/*.{js,jsx}'",
    "test": "node_modules/.bin/mocha --require ts-node/register --extensions js,jsx,ts,tsx tests 'tests/**/*.{ts,tsx,js,jsx}'",
    "serve": "firebase serve --only hosting"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeysmorkalov/AngularSandBox.git"
  },
  "keywords": [
    "TypeScript",
    "Noobie"
  ],
  "author": "Alexey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexeysmorkalov/AngularSandBox/issues"
  },
  "homepage": "https://github.com/alexeysmorkalov/AngularSandBox#readme",
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "^3.6.0",
    "favicons-webpack-plugin": "3.0.1",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "8.0.1",
    "sass": "^1.26.9",
    "sass-loader": "^9.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "3.3.12"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "materialize-css": "^1.0.0-rc.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
