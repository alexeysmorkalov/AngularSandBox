{
  "name": "tsandbox",
  "version": "1.0.2",
  "description": "TS sandbox",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "ts_test": "node_modules/.bin/mocha --require ts-node/register --extensions ts,tsx tests 'tests/**/*.{ts,tsx}'",
    "js_test": "node_modules/.bin/mocha --extensions js,jsx tests 'tests/**/*.{js,jsx}'",
    "test": "node_modules/.bin/mocha --require ts-node/register --extensions js,jsx,ts,tsx tests 'tests/**/*.{ts,tsx,js,jsx}'",
    "frun": "firebase serve --only hosting"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeysmorkalov/AngularSandBox.git"
  },
  "keywords": [
    "TypeScript",
    "Noobie"
  ],
  "author": "Alexey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexeysmorkalov/AngularSandBox/issues"
  },
  "homepage": "https://github.com/alexeysmorkalov/AngularSandBox#readme",
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "favicons-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "4.3.0",
    "mocha": "8.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
